{
  "name": "compact-yarn-audit",
  "version": "1.2.0",
  "description": "Presents output from yarn audit in a compact table",
  "engines": {
    "node": "^12.20||^14.13.1||>=16"
  },
  "license": "MIT",
  "homepage": "https://github.com/sverweij/compact-yarn-audit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sverweij/compact-yarn-audit"
  },
  "bugs": {
    "url": "https://github.com/sverweij/compact-yarn-audit/issues"
  },
  "author": "Sander Verweij (https://sverweij.github.io/)",
  "bin": {
    "compact-yarn-audit": "src/index.js"
  },
  "main": "src/index.js",
  "exports": "./src/index.js",
  "type": "module",
  "types": "types/compact-yarn-audit.d.ts",
  "scripts": {
    "format": "prettier --write src/**/*.js *.md",
    "scm:stage": "git add .",
    "test": "mocha src --exit",
    "update-dependencies": "npm-run-all upem:update upem:install format test",
    "upem-outdated": "npm outdated --json --long | upem --dry-run",
    "upem:update": "npm outdated --json --long | upem | pbcopy && pbpaste",
    "upem:install": "npm install",
    "version": "npm-run-all test scm:stage"
  },
  "keywords": [
    "yarn",
    "yarn-audit"
  ],
  "dependencies": {
    "chalk": "^5.0.1",
    "ndjson": "^2.0.0",
    "strip-ansi": "^7.0.1",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "mocha": "10.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "upem": "7.3.0"
  },
  "files": [
    "src/**/*.js",
    "!src/**/*.spec.js",
    "types",
    "package.json",
    "README.md"
  ]
}
